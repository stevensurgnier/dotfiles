Host *
     Compression yes
     ConnectTimeout 200
     ControlPath ~/.ssh/controls/ctl-%r-%h-%p
     ControlMaster auto
     ForwardAgent yes
     ForwardX11 no
     Protocol 2
     EscapeChar !
     GlobalKnownHostsFile /dev/null
     HashKnownHosts yes
     ServerAliveInterval 5
     SendEnv PS1 PROMPT_COMMAND
     User steven

